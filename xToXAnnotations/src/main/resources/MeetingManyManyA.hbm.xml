<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
        default-lazy="true"
        package="by.academy.it.pojos4"
        auto-import="false"
        catalog="my_db3"
>
    <class name="by.academy.it.pojos4.MeetingManyManyA" table="T_MEETING_MANYMANY">
        <id name="mid" type="int">
            <generator class="increment"/>
        </id>
        <set name="employees" table="T_EMPLOYEE_MEETING" inverse="true">
            <key column="F_MID2"/>
            <many-to-many column="F_EID2" class="by.academy.it.pojos4.EmployeeManyManyA"/>
        </set>
        <property name="subject" column="F_SUBJECT" type="string"/>
    </class>
</hibernate-mapping>
